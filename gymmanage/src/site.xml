		<action name="updateSite" class="com.gms.web.action.SiteAction" method="updateSite">
		</action>
		<action name="recoverSite" class="com.gms.web.action.SiteAction" method="recoverSite">
		</action>
		<action name="removeSite" class="com.gms.web.action.SiteAction" method="removeSite">
		</action>
		<action name="querySite" class="com.gms.web.action.SiteAction" method="querySite">
			<result name="success">/manager/getAllSite.jsp</result>
		</action>
		
		
		<!-- 场地预约 -->
		<action name="getAllSiteOrder" class="com.gms.web.action.SiteAction" method="getAllSiteOrder">
			<result name="success">/manager/getAllSiteOrder.jsp</result>
		</action>
		<action name="getDataForAddSiteOrder" class="com.gms.web.action.SiteAction" method="getDataForAddSiteOrder">
			<result name="success">/manager/addSiteOrder.jsp</result>
		</action>
		<action name="addSiteOrderByManager" class="com.gms.web.action.SiteAction" method="addSiteOrderByManager">
		</action>
		<action name="deleteSiteOrder" class="com.gms.web.action.SiteAction" method="deleteSiteOrder">
		</action>
		<action name="getDataForUpdateSiteOrder" class="com.gms.web.action.SiteAction" method="getDataForUpdateSiteOrder">
			<result name="success">/manager/updateSiteOrder.jsp</result>
		</action>
		<action name="updateSiteOrder" class="com.gms.web.action.SiteAction" method="updateSiteOrder">
			<result name="success">/manager/updateSiteOrder.jsp</result>
		</action>
		<action name="getCurrentSiteOrder" class="com.gms.web.action.SiteAction" method="getCurrentSiteOrder">
