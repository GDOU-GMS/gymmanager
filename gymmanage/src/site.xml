<struts>
	<package name="site" namespace="/site" extends="base">
	
		<!--场地类型管理-->
		<action name="addSiteType" class="com.gms.web.action.SiteAction" method="addSiteType">
		</action>
		<action name="getAllSiteType" class="com.gms.web.action.SiteAction" method="getAllSiteType">
			<result name="success">/manager/getAllSiteType.jsp</result>
		</action>
		<action name="deleteSiteType" class="com.gms.web.action.SiteAction" method="deleteSiteType">
		</action>
		<action name="echoData" class="com.gms.web.action.SiteAction" method="echoData">
