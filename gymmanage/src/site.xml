		<action name="deleteSiteUsage" class="com.gms.web.action.SiteAction" method="deleteSiteUsage">
		</action>
		<action name="dealTimeOutSiteUsage" class="com.gms.web.action.SiteAction" method="dealTimeOutSiteUsage">
		</action>
		<action name="deleteAllTimeOutSiteUsage" class="com.gms.web.action.SiteAction" method="deleteAllTimeOutSiteUsage">
		</action>
		<action name="querySiteUsage" class="com.gms.web.action.SiteAction" method="querySiteUsage">
			<result name="success">/manager/getAllSiteUsage.jsp</result>
		</action>
		
		
		<!-- client -->
		<action name="clientGetDataForAddSiteOrder"  class="com.gms.web.action.SiteAction" method="clientGetDataForAddSiteOrder">
