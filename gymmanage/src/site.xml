		<action name="getAllSiteUsage" class="com.gms.web.action.SiteAction" method="getAllSiteUsage">
			<result name="success">/manager/getAllSiteUsage.jsp</result>
		</action>
		<action name="getDataForAddSiteUsage" class="com.gms.web.action.SiteAction" method="getDataForAddSiteUsage">
			<result name="success">/manager/addSiteUsage.jsp</result>
		</action>
		<action name="addSiteUsage" class="com.gms.web.action.SiteAction" method="addSiteUsage">
		</action>
		<action name="getDataForUpdateSiteUsage" class="com.gms.web.action.SiteAction" method="getDataForUpdateSiteUsage">
			<result name="success">/manager/updateSiteUsage.jsp</result>
		</action>
		<action name="updateSiteUsage" class="com.gms.web.action.SiteAction" method="updateSiteUsage">
