<struts>
   <package name="equipmentaction" namespace="/equipmentaction" extends="base">
     
     <!-- 器材管理 -->
     <action name="getAllEquipment" class="com.gms.web.action.EquipmentAction"
      method="getAllEquipment">
        <result name="success">/manager/getAllEquipment.jsp</result>
     </action>
     <action name="getDataForaddEquipment" class="com.gms.web.action.EquipmentAction"
      method="getDataForaddEquipment">
        <result name="success">/manager/addEquipment.jsp</result>
     </action>
     <action name="addEquipment" class="com.gms.web.action.EquipmentAction"
      method="addEquipment">
      <result name="success">/manager/getAllEquipment.jsp</result>
     </action>
     <action name="deleteEquipment" class="com.gms.web.action.EquipmentAction"
     method="deleteEquipment">
     </action>
     <action name="queryEquipment" class="com.gms.web.action.EquipmentAction"
     method="queryEquipment">
      <result name="success">/manager/getAllEquipment.jsp</result>
     </action>
     <action name="updateEquipment" class="com.gms.web.action.EquipmentAction"
     method="updateEquipment">
     </action>
     
     <!-- 器材租借 -->
      <action name="getAllEquipmentRent" class="com.gms.web.action.EquipmentAction"
