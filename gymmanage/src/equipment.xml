        <result name="success">/manager/equipRentQuery.jsp</result>
     </action>
     <action name="getDataForaddequipRentQuery" class="com.gms.web.action.EquipmentAction"
      method="getDataForaddequipRentQuery">
        <result name="success">/manager/addEquipRent.jsp</result>
     </action>
      <action name="addEquipRentInfo" class="com.gms.web.action.EquipmentAction"
      method="addEquipRentInfo">
      <result name="success">/manager/equipRentQuery.jsp</result>
     </action>
     <action name="deleteEquipmentInfo" class="com.gms.web.action.EquipmentAction"
     method="deleteEquipmentInfo">
     </action>
         <action name="updateEquipmentInfo" class="com.gms.web.action.EquipmentAction"
     method="updateEquipmentInfo">
     </action>
      <action name="queryEquipmentRent" class="com.gms.web.action.EquipmentAction"
     method="queryEquipmentRent">
      <result name="success">/manager/equipRentQuery.jsp</result>
     </action>
     
     <!-- 器材收费标标准设置 -->
     <action name="getAllEquipmentCharge" class="com.gms.web.action.EquipmentAction"
      method="getAllEquipmentCharge">
        <result name="success">/manager/getAllEquipmentCharge.jsp</result>
     </action>
       <action name="deleteEquipmentCharge" class="com.gms.web.action.EquipmentAction"
     method="deleteEquipmentCharge">
     </action>
     <action name="getDataForsetEquipmentCharge" class="com.gms.web.action.EquipmentAction"
      method="getDataForsetEquipmentCharge">
        <result name="success">/manager/setEquipmentCharge.jsp</result>
     </action>
      <action name="setEquipmentCharge" class="com.gms.web.action.EquipmentAction"
      method="setEquipmentCharge">
     </action>
      <action name="updateEquipmentCharge" class="com.gms.web.action.EquipmentAction"
     method="updateEquipmentCharge">
     </action>
        <action name="queryEquipmentCharge" class="com.gms.web.action.EquipmentAction"
     method="queryEquipmentCharge">
     </action>
   </package>
   
  
</struts>